<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.onlinequiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.login" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_1" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_1" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_2" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_2" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_3" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_3" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_4" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_4" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_5" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_5" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".page_final" />

        <intent-filter>
            <action android:name="com.example.android.onlinequiz.page_final" />

            <category android:name="android.intent.category.DEFAULT" />
        </intent-filter>

        <activity android:name=".register_page">
            <intent-filter>
                <action android:name="com.example.android.onlinequiz.register_page" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Show_all_score"></activity>
    </application>

</manifest>